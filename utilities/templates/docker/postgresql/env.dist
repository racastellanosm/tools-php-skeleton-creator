# Application
APP_NAME=project_name_placeholder
APP_ENV=dev
APP_SECRET=
APP_MESSENGER_TRANSPORT_DSN=doctrine://default?auto_setup=0
APP_DATABASE_DSN=postgresql://my_user:my_password@postgresql:5432/my_app?serverVersion=17&charset=utf8
APP_DEBUG=1
APP_RUNTIME=Runtime\RoadRunnerSymfonyNyholm\Runtime
APP_LOG_LEVEL=info

# PostgreSQL
POSTGRES_USER=my_user
POSTGRES_PASSWORD=my_password
POSTGRES_DB=my_app

# RoadRunner
APP_NUM_WORKERS=1
APP_MAX_JOBS=100
APP_MAX_WORKER_MEMORY=128